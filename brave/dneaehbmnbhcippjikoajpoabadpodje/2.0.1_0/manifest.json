{
   "content_scripts": [ {
      "css": [ "styles.css" ],
      "matches": [ "https://old.reddit.com/*" ]
   } ],
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "ruleset_1",
         "path": "rules.json"
      } ]
   },
   "description": "Ensure Reddit always loads the old design",
   "host_permissions": [ "*://reddit.com/*", "*://www.reddit.com/*", "*://np.reddit.com/*", "*://amp.reddit.com/*", "*://i.reddit.com/*", "*://i.redd.it/*", "*://preview.redd.it/*" ],
   "icons": {
      "128": "img/icon128.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Old Reddit Redirect",
   "permissions": [ "declarativeNetRequestWithHostAccess" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.1"
}
